{
  "name": "redux-action-namespace",
  "version": "1.0.0",
  "description": "redux actions with inherited classes",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "types": "./lib/cjs/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmanuel1609/redux-action-namespace.git"
  },
  "keywords": [
    "Redux",
    "Actions",
    "Namespaces",
    "Classes"
  ],
  "author": "Jesus Roblejo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmanuel1609/redux-action-namespace/issues"
  },
  "homepage": "https://github.com/jmanuel1609/redux-action-namespace#readme",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "move-file-cli": "^3.0.0",
    "semantic-release": "^23.0.2",
    "serialize-error": "^11.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
